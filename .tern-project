{
  "ecmaVersion": 8,
  "libs": [
    "browser"
  ],
  "plugins": {
    "node_resolve": {},
    "es_modules": {},
    "webpack": {
      "alias": {
        "@app":  "./src",
        "@core":  "./src/core",
        "@test":  "./src/test",
        "@lib":  "./lib",
        "@res":  "./res",
      },
      "configPath": "./webpack.config.js",
      "modules": ["node_modules", "src", "lib"]
    }
  }
}
