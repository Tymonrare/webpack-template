{
  "ecmaVersion": 8,
  "libs": [
    "browser"
  ],
  "plugins": {
    "node_resolve": {},
    "es_modules": {},
    "webpack": {
      "alias": {
        "@app":  "./src",
        "@resources":  "./src/resources",
        "@devcode":  "./src/dev",
        "@css":  "./src/css",
        "@tests":  "./src/_test_",
        "@backend":  "./src/backend",
        "@requests": "./src/requests",
        "@gui": "./src/gui"
      },
      "configPath": "./webpack.config.js",
      "modules": ["node_modules", "src"]
    }
  }
}
