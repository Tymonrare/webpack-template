{
  "name": "webpack_template",
  "version": "1.0.0",
  "description": "Default webpack template for HTML projects",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "src": "src",
    "res": "res"
  },
  "scripts": {
    "unit-test": "karma start karma.config.js --single-run",
    "unit-test-watch": "webpack-dev-server --open --env.test=unit --env.chunks=true --mode=development",
    "manual-test-watch": "webpack-dev-server --open --env.test=manual --env.chunks=true --mode=development",
    "manual-test-build": "webpack --config webpack.config.js --env.test=manual --mode=development",
    "build-dev": "webpack --config webpack.config.js --mode=development",
    "build-prod": "webpack --config webpack.config.js --mode=production",
    "watch": "webpack-dev-server --open --mode=development",
    "docs": "jsdoc ./src ./README.md -c jsdoc.json --pedantic -r -d docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^6.2.2",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-jsdoc": "^15.8.3",
    "html-webpack-plugin": "^3.2.0",
    "inline-source-map": "^0.6.2",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^4.0.2",
    "puppeteer": "^2.0.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.0",
    "mocha-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
